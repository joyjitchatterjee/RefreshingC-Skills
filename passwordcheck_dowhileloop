using namespace std;

int main()

{  string correctPass = "1996";
   string userInput = "";
   
   do{
       cout<<"Hi. Please key in the password to access:\n";
       cin>>userInput;
   }while(userInput!=correctPass);
   
   cout<<"Access granted.";
   return 0;
}
