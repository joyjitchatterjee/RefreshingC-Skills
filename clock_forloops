#include <iostream>
#include<iomanip>
using namespace std;

int main()

{   cout<<"Welcome to the clock Program utilising For-Loops!\n";
    cout<<"We would display hh:mm:ss in this program :-)\n";
    
    for(int h=0;h<24;h++)
    {
        for(int m=0;m<60;m++)
        {
            for(int s=0;s<60;s++)
            {
                cout<<h<<":"<<m<<":"<<s<<endl;
            }
        }
    }
    return 0;
}
